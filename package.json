{
  "name": "eslint-plugin-better-mutation",
  "version": "1.5.0",
  "description": "ESLint rules for controlling where and how mutation is used.",
  "license": "MIT",
  "repository": "sloops77/eslint-plugin-better-mutation",
  "author": {
    "name": "Andres Olave",
    "email": "andres@thinkingbytes.net",
    "url": "https://github.com/sloops77"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "init:ci": "rimraf reports && mkdir -p reports",
    "lint": "xo",
    "lint:ci": "xo --reporter junit > reports/lint-results.xml",
    "test": "nyc ava",
    "test:ci": "nyc --reporter=lcov ava --tap | tap-xunit > reports/test-results.xml"
  },
  "files": [
    "index.js",
    "rules"
  ],
  "keywords": [
    "eslint",
    "plugin",
    "eslint-plugin",
    "eslintplugin",
    "fp",
    "FP",
    "function",
    "functional",
    "programming",
    "lodash",
    "mutation",
    "immutable",
    "function",
    "functions",
    "method",
    "methods"
  ],
  "dependencies": {
    "create-eslint-index": "~1.0.0",
    "debug": "~4.3.5",
    "eslint-ast-utils": "~1.1.0",
    "import-modules": "~3.2.0",
    "lodash": "~4.17.21"
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "ava": "~6.1.3",
    "eslint": "~7.15.0",
    "eslint-ava-rule-tester": "~4.2.0",
    "eslint-plugin-eslint-plugin": "~6.3.2",
    "eslint-plugin-n": "~17.13.1",
    "inject-in-tag": "~1.1.1",
    "nyc": "~17.1.0",
    "pify": "~6.1.0",
    "rimraf": "~6.0.1",
    "tap-xunit": "~2.4.1",
    "xo": "~0.56.0"
  },
  "peerDependencies": {
    "eslint": ">=6"
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "extends": [
      "plugin:eslint-plugin/recommended",
      "plugin:n/recommended"
    ],
    "rules": {
      "unicorn/prefer-module": "off",
      "import/extensions": "off",
      "eslint-plugin/require-meta-docs-description": "error"
    }
  },
  "ava": {
    "babel": true
  },
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
}
